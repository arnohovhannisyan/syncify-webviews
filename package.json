    "@types/jquery": "^3.3.31",
    "@types/lodash": "^4.14.146",
    "@types/marked": "^0.7.1",
    "@types/node": "^12.12.7",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "fs-extra": "^8.1.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "sass": "^1.23.3",
    "shx": "^0.3.2",
    "tslib": "^1.10.0",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.2"
  },
