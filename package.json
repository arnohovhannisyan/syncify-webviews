{
  "name": "syncify-webviews",
  "version": "2.0.0",
  "description": "A reliable way of syncing your VSCode settings and extensions",
  "license": "MIT",
  "scripts": {
    "start": "yarn clean && parcel src/index.html",
    "build": "yarn clean && parcel build src/index.html --public-url . --no-source-maps && yarn postprocess",
    "clean": "shx rm -rf dist",
    "postprocess": "node scripts/postprocess.js"
  },
  "devDependencies": {
    "@types/bootstrap": "^4.3.1",
    "@types/dompurify": "^2.0.1",
    "@types/fs-extra": "^8.0.1",
    "@types/lodash": "^4.14.149",
    "@types/marked": "^0.7.2",
    "@types/node": "^13.7.0",
    "fs-extra": "^8.1.0",
    "parcel": "^1.12.4",
    "prettier": "^1.19.1",
    "sass": "^1.25.0",
    "shx": "^0.3.2",
    "tslib": "^1.10.0",
    "tslint": "^6.0.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "bootstrap": "4.4.1",
    "dompurify": "^2.0.8",
    "fuse.js": "^3.4.6",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "marked": "^0.8.0",
    "preact": "^10.3.1",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "^16.12.0",
    "react-use": "^13.24.0",
    "rxjs": "^6.5.4"
  },
  "browserslist": [
    "last 10 Chrome versions"
  ],
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat"
  }
}
